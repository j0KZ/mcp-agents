{
  "$schema": "https://raw.githubusercontent.com/anthropics/claude-desktop/main/config.schema.json",
  "mcpServers": {
    "j0kz-mcp-gateway": {
      "command": "docker",
      "args": [
        "mcp",
        "gateway",
        "run",
        "--config",
        "/path/to/mcp-agents/docker/mcp-config/servers.json"
      ],
      "env": {
        "DOCKER_HOST": "unix:///var/run/docker.sock"
      }
    }
  }
}
